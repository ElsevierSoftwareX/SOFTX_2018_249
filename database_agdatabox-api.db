CREATE FUNCTION public.f_atualizaarea() RETURNS trigger
    LANGUAGE plpgsql
    AS $$

declare
  v_tamanho  float;
  local_srid integer;
  v_longcentral float;
  v_latcentral float;
  v_codarea integer;
begin
       v_codarea := NEW.codigo;
       raise notice 'o codigo da are é: %', v_codarea;
       execute 'select st_x(st_centroid(geometry)), st_y(st_centroid(geometry)) from area
               where codigo = '||v_codarea into v_longcentral, v_latcentral;
      select f_buscadatum_utm(v_longcentral, v_latcentral) into local_srid;
      execute 'select st_area(st_transform(geometry, '||local_srid||'))/10000 from area 
               where codigo = '||v_codarea into v_tamanho;
     execute 'update area set tamanho = '||v_tamanho||' where codigo = '||v_codarea;
     return NULL;
end;
$$;


