--
-- TOC entry 354 (class 1255 OID 20902)
-- Name: f_verificapermissaodelete(text, integer, integer, integer); Type: FUNCTION; Schema: public; Owner: api
--

CREATE FUNCTION public.f_verificapermissaodelete(p_objeto text, p_autorizado integer, p_autorizante integer, p_codigolinha integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
declare
   v_tipoobjeto varchar(60);
   v_permissao  integer default null;
   v_id_validos integer[] ;
   v_retorno boolean default false;
   V_nomecolunaID varchar(60);
   V_colunaautorizante varchar(60);
   v_sql text;
   v_posicao integer default 0;
begin
     select obj_visibilidade
         from tb_objeto where Obj_nomeobjetobanco = p_objeto into v_tipoobjeto;
     if v_tipoobjeto = 'IRRESTRITA' then
        return 0;    
     else
        select per_codigo from v_permissaodelete where per_autorizadocodigo = p_autorizado
                                                 and   per_autorizantecodigo = p_autorizante
                                                 and   obj_nomeobjetobanco = p_objeto into v_permissao;
        if v_permissao is null then
           return -1;
        else
          select Obj_chaveprimaria, Obj_colunaidusuario from tb_objeto where upper(obj_nomeobjetobanco) = upper(p_objeto) into v_nomecolunaID, V_colunaautorizante; 
          v_sql := 'select array_agg('||v_nomecolunaID||') from '|| p_objeto||'  where '||V_colunaautorizante||' = '||p_autorizante;
          execute v_sql into v_id_validos; 
          if v_id_validos is null then
             v_id_validos := '{0}';
          end if;
	  for i in 1..array_upper(v_id_validos, 1) loop
                if v_id_validos[i] = p_codigolinha then
                   v_posicao := i;
                   exit;
                end if;
	  end loop;
          if v_posicao <> 0 then     
              return v_permissao;
            else 
              return -1;
            end if;
        end if;
     end if;
end;
$$;


ALTER FUNCTION public.f_verificapermissaodelete(p_objeto text, p_autorizado integer, p_autorizante integer, p_codigolinha integer) OWNER TO api;

